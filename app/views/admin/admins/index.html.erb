<h1>Admins</h1>

<table>
  <thead>
    <tr>
      <th>email</th>
      <th>sign_in_count</th>
      <th>last_sign_in_at</th>
    </tr>
  </thead>
  <tbody>
    <% @admins.each do |admin| %>
      <tr>
        <td><%= admin.email %></td>
        <td><%= admin.sign_in_count %></td>
        <td><%= time_ago_in_words admin.last_sign_in_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
